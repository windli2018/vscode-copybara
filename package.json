{
  "name": "vscode-copybara",
  "displayName": "Copybara",
  "description": "Syntax highlighting and IntelliSense for Copybara configuration files (.sky)",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "publisher": "windli2018",
  "repository": {
    "type": "git",
    "url": "https://github.com/windli2018/vscode-copybara.git"
  },
  "author": {
    "name": "windli2018"
  },
  "license": "Apache-2.0",
  "icon": "media/copybara-icon.png",
  "contributes": {
    "languages": [{
      "id": "copybara",
      "aliases": ["Copybara", "copybara"],
      "extensions": [".sky"],
      "filenames": ["copy.bara.sky"],
      "configuration": "./language-configuration.json",
      "icon": {
        "dark": "./media/copybara-icon.png",
        "light": "./media/copybara-icon.png"
      }
    }],
    "grammars": [{
      "language": "copybara",
      "scopeName": "source.copybara",
      "path": "./syntaxes/copybara.tmLanguage.json"
    }],
    "snippets": [{
      "language": "copybara",
      "path": "./snippets/copybara.json"
    }]
  }
}
